{
    "name": "routegroup/laravel-imoje",
    "description": "Package for imoje payments",
    "keywords": [
        "routegroup",
        "laravel",
        "imoje"
    ],
    "homepage": "https://github.com/routegroup/laravel-imoje",
    "license": "MIT",
    "authors": [
        {
            "name": "Kajetan Nobel",
            "email": "k.nobel@routegroup.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^10.0|^11.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Routegroup\\Imoje\\Payment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Routegroup\\Imoje\\Payment\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-imoje --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Routegroup\\Imoje\\Payment\\ImojeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
